<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @project.name %>
</p>


<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>

<h1>Listing iterations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Start</th>
    <th>End</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iterations.each do |iteration| %>
  <tr>
    <td><%= iteration.name %></td>
    <td><%= iteration.start %></td>
    <td><%= iteration.end %></td>
    <td><%= link_to 'Show', project_iteration_path(@project, iteration) %></td>
    <td><%= link_to 'Edit', edit_project_iteration_path(@project, iteration) %></td>
    <td><%= link_to 'Destroy', project_iteration_path(@project, iteration), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Iteration', new_project_iteration_path(@project) %>
